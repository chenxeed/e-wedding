  "scripts": {
    "dev": "svelte-kit dev",
    "build": "svelte-kit build",
    "gh-pages-clean": "gh-pages-clean",
    "build-gh": "svelte-kit build -- paths=/kelvinrita && gh-pages -d build -t true",
    "preview": "svelte-kit preview",
